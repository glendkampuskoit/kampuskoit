- provide(:title, "Data Provinsi")
%a.btn.btn-info.btn-small.small-margin-bottom{:href => "#{ new_provinsi_path }"} Tambah Provinsi
.relativeWrap
  .widget.widget-gray.widget-gray-white
    .widget-head
      %h4.heading Data Provinsi
    .widget-body
      %table.dynamicTable.table.table-striped.table-bordered.table-primary.table-condensed{:border => "0", :cellpadding => "0", :cellspacing => "0"}
        %thead
          %tr
            %th{:width => "350"} Nama Provinsi
            %th{:width => "50"} &nbsp;
        %tbody
          - @provinsis.each do |provinsi|
            %tr.gradeX
              %td= provinsi.provinsi
              %td.center.actions
                %a.btn-action.glyphicons.pencil.btn-success{:href => "#{ edit_provinsi_path(provinsi) }"}
                  %i
                %a.btn-action.glyphicons.remove_2.btn-danger{:href => "#{ provinsi_path(provinsi) }", method: :delete}
                  %i