<% provide(:title, "Jenjang Jurusan") %>
<table class="table table-striped table-bordered">
   <thead>
      <tr>
         <th>No.</th>
         <th>Jenjang Jurusan</th>
         <th>Keterangan</th>
         <th></th>
      </tr>
   </thead>
   <tbody>
   		<% no = 0 %>
      <% @jenjang_prodis.each do |jenjang_prodi| %>
      <tr>
         <td><%= no = no + 1 %></td>
         <td><%= jenjang_prodi.jenjang %></td>
         <td><%= jenjang_prodi.keterangan %></td>
         <td>
         	<div class="btn-group pull-right">
              <%= link_to edit_jenjang_prodi_path(jenjang_prodi), :role => "button", :class => "btn btn-info" do %><i class="icon-edit glyphw"></i> Edit <% end %>
             <%= link_to jenjang_prodi, method: :delete, :role => "button", :class => "btn btn-danger" do %><i class="icon-trash glyphw"></i> Hapus <% end %>
          </div>
         </td>
      </tr>
      <% end %>
   </tbody>
</table>
