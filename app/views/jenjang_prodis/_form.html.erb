   <%= form_for @jenjang_prodi do |f| %>
   <fieldset>
      <div id="legend">
         <legend class=""><%= yield(:title) %></legend>
      </div>
      <% if @jenjang_prodi.errors.any? %>
         <div class="alert alert-error">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <div>
               <% @jenjang_prodi.errors.full_messages.each do |msg| %>
               <%= msg %><br>
               <% end %>
            </div>
         </div>
      <% end %>
      <div class="control-group">
         <%= f.label "Jenjang:", :class => "control-label" %>
         <div class="controls">
            <%= f.text_field :jenjang, :class => "input-xlarge", :placeholder => "Jenjang Prodi" %>
            <p class="help-block">Masukkan jenjang jurusan</p>
         </div>
      </div>
      <div class="control-group">
         <%= f.label "Keterangan:", :class => "control-label" %>
         <div class="controls">
            <%= f.text_field :keterangan, :class => "input-xlarge", :placeholder => "Keterangan" %>
            <p class="help-block">Masukkan keterangan jenjang</p>
         </div>
      </div>
      <div class="control-group">
         <!-- Button -->
         <div class="controls">
            <button class="btn btn-primary">Save</button>
            <button class="btn" type="reset">Reset</button>
         </div>
      </div>
   </fieldset>
   <% end %>  