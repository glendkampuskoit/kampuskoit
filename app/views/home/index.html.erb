  <title>Selamat Datang di Kampus.co.id</title>
  
  <link href='http://fonts.googleapis.com/css?family=Alef' rel='stylesheet' type='text/css'>
  
  <link rel="stylesheet" href="<%= asset_path('front/normalize.css') %>" />  
  <link rel="stylesheet" href="<%= asset_path('front/foundation-kampus01.css') %>" />  
  <link rel="stylesheet" href="<%= asset_path('front/website-kampus01.css') %>" /> 
  <script src="<%= asset_path ('vendor/custom.modernizr.js') %>"></script>

</head>
<body>
  <header>
    <nav>      
      <ul>
        <li><a href="#">Blog</a></li>
        <li><a href="#">Rating</a></li>
        <li><a href="#">Login</a></li>
      </ul>
    </nav>
  </header>
  <div role="main">
    <div class="row">
      <div class="small-5 small-centered columns logo-fix">
        <img class="double-fix" src="assets/images/website-kampus01/logo.png">
      </div>
    </div>
    <div class="row">
      <div class="small-7 small-centered columns">
        <form>
          <fieldset>
            <div class="row">
              <div class="large-12 columns">                
                <input type="text" class="custom-input">                
              </div>
            </div>
            <div class="row">
              <div class="large-12 columns">
                <ul class="button-group">
                  <li class="width-20 start-round"><a class="active-button small button custom-border start-round">Semua</a></li>
                  <li class="width-40"><a class="small button custom-border">Perguruan Tinggi</a></li>
                  <li class="width-20"><a class="small button custom-border">Jurusan</a></li>
